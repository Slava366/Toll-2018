<?xml version="1.0" encoding="UTF-8"?>

<configuration>

    <appender name="DEFAULT_STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{dd.MM.yy HH:mm:ss.SSS} [%thread] %-5level %logger{5} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="INFO">
        <appender-ref ref="DEFAULT_STDOUT"/>
    </root>

</configuration>
